{
  "name": "wenfeng/hyperf-ext-telegram",
  "description": "A telegram reusable Hyperf module template with JWT and routes",
  "type": "library",
  "license": "MIT",
  "version": "3.2",
  "require": {
    "php": ">=8.3",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-redis": "*"
  },
  "require-dev": {
    "hyperf/cache": "~3.1.0",
    "hyperf/command": "~3.1.0",
    "hyperf/config": "~3.1.0",
    "hyperf/constants": "^3.1",
    "hyperf/database": "~3.1.0",
    "hyperf/db-connection": "~3.1.0",
    "hyperf/engine": "^2.10",
    "hyperf/framework": "~3.1.0",
    "hyperf/guzzle": "~3.1.0",
    "hyperf/http-server": "~3.1.0",
    "hyperf/logger": "~3.1.0",
    "hyperf/memory": "~3.1.0",
    "hyperf/paginator": "^3.1",
    "hyperf/process": "~3.1.0",
    "hyperf/translation": "^3.1",
    "irazasyed/telegram-bot-sdk": "^3.15",
    "kornrunner/secp256k1": "^0.3.0",
    "phper666/jwt-auth": "^4.0",
    "simplito/elliptic-php": "^1.0",
    "stephenhill/base58": "^1.1",
    "hyperf/devtool": "~3.1.0",
    "hyperf/testing": "~3.1.0",
    "mockery/mockery": "^1.0",
    "hyperf/swagger": "^3.1",
    "swoole/ide-helper": "^5.0",
    "hyperf/async-queue": "^3.1",
    "hyperf/redis": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "William\\HyperfExtTelegram\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "./test/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "hyperf": {
      "config": "William\\HyperfExtTelegram\\ConfigProvider"
    }
  },
  "scripts": {
    "test": "co-phpunit --prepend test/bootstrap.php --colors=always"
  }
}
